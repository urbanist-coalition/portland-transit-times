{
  "name": "portland-me-transit-times",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "capture-predictions": "tsx src/build-scripts/generate-stop-name-overrides.ts --refresh",
    "generate-stop-name-overrides": "tsx src/build-scripts/generate-stop-name-overrides.ts",
    "generate-json": "npm run generate-stop-name-overrides && tsx src/build-scripts/generate-json.ts",
    "build": "npm run generate-json && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.1.10",
    "@mui/material": "^6.1.10",
    "@mui/material-nextjs": "^6.1.9",
    "@next/third-parties": "^15.1.0",
    "ajv": "^8.17.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "next": "15.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-transition-group": "^4.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check \"src/**/*.{ts,tsx}\""
    }
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "husky": "^9.1.7",
    "json-stable-stringify": "^1.2.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "es5"
  }
}
